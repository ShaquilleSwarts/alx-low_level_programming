#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n =10;
  char csize ='a';
  long int li =10;
  long long int lli =10;
  float fl =10.0;
  printf("Size of a char: %zu byte(s)\n",csize);
  printf("Size of an int: %zu byte(s)\n",n);
  printf("Size of a long int: %zu byte(s)\n",li);
  printf("Size of a long long int: %zu byte(s)\n",lli);
  printf("Size of a float: %zu byte(s)\n",fl);
  return (0);
}
